{
    "name": "teddycloud-web",
    "version": "0.6.0",
    "private": true,
    "homepage": "/web",
    "dependencies": {
        "@ant-design/icons": "^5.3.7",
        "@dnd-kit/core": "^6.1.0",
        "@dnd-kit/sortable": "^8.0.0",
        "@openapitools/openapi-generator-cli": "^2.7.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.39",
        "@types/react": "^18.2.17",
        "@types/react-dom": "^18.2.7",
        "@types/w3c-web-serial": "^1.0.6",
        "antd": "^5.19.0",
        "dnd-kit": "^0.0.2",
        "esptool-js": "^0.4.5",
        "formik": "^2.4.2",
        "i18next": "^23.3.0",
        "i18next-browser-languagedetector": "^7.1.0",
        "i18next-http-backend": "^2.2.1",
        "react": "^18.2.0",
        "react-dom": "^18.3.1",
        "react-i18next": "^13.0.2",
        "react-router-dom": "^6.24.1",
        "react-scripts": "5.0.1",
        "react-syntax-highlighter": "^15.5.0",
        "styled-components": "^6.0.5",
        "typescript": "^4.9.5",
        "yup": "^1.2.0"
    },
    "scripts": {
        "build": "react-scripts build",
        "start-http": "cross-env PORT=3000 react-scripts start",
        "start-https": "cross-env HTTPS=true PORT=3443 react-scripts start",
        "start": "concurrently \"npm run start-http\" \"npm run start-https\"",
        "api:generate": "rm -rf ./src/api && openapi-generator-cli generate -i ./api/swagger.yaml -g typescript-fetch -o ./src/api --additional-properties=typescriptThreePlus=true",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 120
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@types/react-syntax-highlighter": "^15.5.13",
        "cross-env": "^7.0.3"
    }
}
