{
    "name": "teddycloud-web",
    "version": "0.6.3",
    "type": "module",
    "private": true,
    "homepage": "/web",
    "dependencies": {
        "@ant-design/icons": "5.5.1",
        "@dnd-kit/core": "6.1.0",
        "@dnd-kit/sortable": "8.0.0",
        "@mebtte/react-media-session": "1.1.2",
        "@openapitools/openapi-generator-cli": "2.14.0",
        "@types/jest": "29.5.13",
        "@types/node": "22.7.5",
        "@types/react": "18.3.11",
        "@types/react-dom": "18.3.1",
        "@types/w3c-web-serial": "1.0.7",
        "@vitejs/plugin-react": "^4.3.3",
        "@vitejs/plugin-react-swc": "3.7.0",
        "antd": "5.21.4",
        "dnd-kit": "0.0.2",
        "esptool-js": "0.4.7",
        "formik": "2.4.6",
        "i18next": "23.16.0",
        "i18next-browser-languagedetector": "8.0.0",
        "i18next-http-backend": "2.6.2",
        "npm-check-updates": "17.1.3",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-i18next": "15.0.3",
        "react-router-dom": "6.27.0",
        "react-syntax-highlighter": "15.6.1",
        "styled-components": "6.1.13",
        "typescript": "5.5.2",
        "vite": "5.3.1",
        "vite-plugin-svgr": "4.2.0",
        "vite-tsconfig-paths": "4.3.2",
        "yup": "1.4.0"
    },
    "overrides": {
        "typescript": "5.5.2",
        "postcss": "8.4.31"
    },
    "devDependencies": {
        "@babel/plugin-transform-private-property-in-object": "7.25.8",
        "@types/react-syntax-highlighter": "15.5.13",
        "@vitest/coverage-v8": "1.6.0",
        "concurrently": "8.2.2",
        "cross-env": "7.0.3",
        "jsdom": "24.1.0",
        "vitest": "1.6.0"
    },
    "scripts": {
        "serve": "vite preview",
        "api:generate": "rm -rf ./src/api && openapi-generator-cli generate -i ./api/swagger.yaml -g typescript-fetch -o ./src/api --additional-properties=typescriptThreePlus=true",
        "test": "vitest",
        "test:coverage": "vitest run --coverage --watch=false",
        "lint": "npx eslint --max-warnings=0 .",
        "start-http": "cross-env PORT=3000 vite",
        "start-https": "cross-env HTTPS=true PORT=3443 vite",
        "start": "concurrently \"npm run start-http\" \"npm run start-https\"",
        "build": "tsc && vite build",
        "preview": "vite preview"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 120
    }
}
