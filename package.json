{
    "name": "teddycloud-web",
    "version": "0.6.0",
    "private": true,
    "homepage": "/web",
    "dependencies": {
        "@ant-design/icons": "5.5.1",
        "@dnd-kit/core": "6.1.0",
        "@dnd-kit/sortable": "8.0.0",
        "@mebtte/react-media-session": "1.1.2",
        "@openapitools/openapi-generator-cli": "2.15.3",
        "@types/jest": "29.5.14",
        "@types/node": "22.9.0",
        "@types/react": "18.3.12",
        "@types/react-dom": "18.3.1",
        "@types/w3c-web-serial": "1.0.6",
        "@vitejs/plugin-react": "4.3.1",
        "antd": "5.22.0",
        "dnd-kit": "0.0.2",
        "formik": "2.4.6",
        "i18next": "23.16.5",
        "i18next-browser-languagedetector": "8.0.0",
        "i18next-http-backend": "2.6.2",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-i18next": "15.1.1",
        "react-syntax-highlighter": "15.5.0",
        "react-router-dom": "6.28.0",
        "styled-components": "6.1.13",
        "typescript": "5.5.2",
        "vite": "^5.4.0",
        "vite-plugin-svgr": "^4.2.0",
        "vite-tsconfig-paths": "^5.0.1",
        "yup": "1.4.0"
    },
    "overrides": {
        "typescript": "5.5.2",
        "postcss": "8.4.31"
    },
    "devDependencies": {
        "@babel/plugin-transform-private-property-in-object": "7.25.9",
        "react-scripts": "5.0.1"
    },
    "scripts": {
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint": "npx eslint --max-warnings=0 ."
        "start-http": "cross-env PORT=3000 vite",
        "start-https": "cross-env HTTPS=true PORT=3443 vite",
        "start": "concurrently \"npm run start-http\" \"npm run start-https\"",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "api:generate": "rm -rf ./src/api && openapi-generator-cli generate -i ./api/swagger.yaml -g typescript-fetch -o ./src/api --additional-properties=typescriptThreePlus=true"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 120
    },
    "devDependencies": {
        "@babel/plugin-transform-private-property-in-object": "7.24.7",
        "@types/react-syntax-highlighter": "15.5.13",
        "concurrently": "8.2.2",
        "cross-env": "7.0.3"
    },
    "type": "module"
}
